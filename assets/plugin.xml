<?xml version="1.0" encoding="UTF-8"?>
<!-- 插件提供的功能(类) -->
<plugin-features>
	<!-- 描述类，这个是自定义的 -->
	<description name="com.mogu3.app.wei.PlugDescription"/>
	<intent action="com.mogu3.app.wei.PlugActivity" key="main">启动.</intent>
	<!-- 这是一个功能(类) -->
	<feature name="com.mogu3.app.wei.PlugActivity">
	
		<!-- 这是一个方法 -->
		<!-- name 方法名 -->
		<method need-context="true" name="callPhone" >插入一条数据</method>
		<method need-context="true" name="callWeb" >创建数据库</method>
		
	</feature>
	<!-- url 更新 同步 -->
	<url main="http://main2.mmggoo.com">
		<uri type="infoall" class="com.mogu3.app.wei.service.WeiUrlSync">/InfoAll</uri>
		<uri type="infoupdate"  class="com.mogu3.app.wei.service.WeiUrlSync">/InfoUpdate</uri>
	</url>
	<code>a4-s2</code>
	<type>game</type>
	<name>微群插件</name>
</plugin-features>